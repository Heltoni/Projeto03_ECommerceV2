
@{
    ViewBag.Title = "Sucesso";
}

<h2>Sucesso</h2>

<div class="alert alert-success">
    @Html.Raw(ViewBag.MensagemSucesso)
</div>

@if (ViewBag.VoltarListaProduto == true || ViewBag.VoltarListaProduto != null)
{
    <div>
        @Html.ActionLink("Voltar para a lista de produtos", "Index")
    </div>
}